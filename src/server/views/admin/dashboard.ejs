<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script src="/auth/js/getAuthUserInfo.js"></script>
    <script src="/admin/js/dashboard.js"></script>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <script
      defer
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
  </head>
  <body class="container p-5">
    <%- include('../_partials/logout.ejs'); %> <%-
    include('../_partials/breadcrumb.ejs') %>
    <h1>Admin Dashboard</h1>
    <a href="/admin/event/new" id="create-event-btn" class="btn btn-success"
      >Create Event</a
    >
    <ul>
      <% events.forEach(event => { %>

      <!-- Item Design Start -->
      <li>
        <a href="/event/<%= event._id %>/dashboard">
          <h3><%= event.name %></h3>
        </a>
      </li>
      <!-- Item Design End -->

      <% }) %>
      <!-- Item Design Start -->
      <!--
      <li>
        <a href="">
          <h3>Event Name</h3>
        </a>
      </li>
      -->
      <!-- Item Design End -->
    </ul>
  </body>
</html>
